{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "types": ["cypress", "node"],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["es5", "dom"],
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}